::-webkit-scrollbar {
    width: 0;
    height: 0;
    color: transparent;
}

page {
    min-height: 100%;
    display: flex;
    flex-direction: column;
    background-color: white;
    padding-bottom: constant(safe-area-inset-bottom);
    padding-bottom: env(safe-area-inset-bottom);
}

.rel {
    top: 0;
    position: fixed;
    z-index: 999;
    width: 100%;
}

.content-view {
    background: white;
    top: -6px;
    position: relative;
    border-bottom-width: 66px;
}
.my-btnf {
    position: fixed;
    right: 50rpx;
    top: 1100rpx;
    z-index: 900;
}
.select-my{
    background-color: black;
    z-index: 2000;
    position: absolute;
}
